{
  "name": "feedbase",
  "version": "0.3.1",
  "scripts": {
    "initialize": "npm i && npm run build",
    "build": "npm run build:ts && npm run build:sol",
    "build:ts": "npx tsc -b",
    "build:sol": "npx hardhat compile",
    "pretest": "npm run build",
    "test": "ts-mocha test/*.ts test/**/*.ts",
    "fmt": "ts-standard --fix src task test"
  },
  "main": "./dist/scripts/index.js",
  "dependencies": {
    "debug": "^4.3.4",
    "ethers-eip712": "^0.2.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@etherpacks/dpack": "^0.0.31",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "hardhat": "^2.12.6",
    "minihat": "^0.0.6",
    "ts-mocha": "^10.0.0",
    "ts-standard": "^12.0.2",
    "typescript": "^4.9.4"
  }
}
