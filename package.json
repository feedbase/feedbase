{
  "name": "feedbase",
  "version": "0.2.2",
  "scripts": {
    "build": "npm run build:ts && npm run build:sol",
    "build:ts": "tsc && chmod +x dist/src/cmd.js",
    "build:sol": "npx hardhat compile",
    "pretest": "npm run build",
    "test": "ts-mocha test/**/*.ts",
    "fmt": "ts-standard --fix src task test"
  },
  "bin": {
    "feed": "./dist/src/cmd.js"
  },
  "main": "./dist/src/index.js",
  "dependencies": {
    "commander": "^8.0.0",
    "debug": "^4.3.2",
    "ethers": ">=5.4.6 <5.5.0",
    "ethers-eip712": "^0.2.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "hardhat": "^2.4.3",
    "ts-mocha": "^8.0.0",
    "ts-standard": "^10.0.0",
    "typescript": "^4.4.2"
  }
}
