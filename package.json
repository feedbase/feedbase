{
  "name": "feedbase",
  "version": "0.1.12",
  "scripts": {
    "build": "npm run build:ts && npm run build:sol",
    "build:ts": "npm run clean:ts && tsc && chmod +x dist/src/cmd.js",
    "clean:ts": "rm -r dist/* && mkdir dist/dpacks",
    "build:sol": "npx hardhat compile",
    "build:docs:sol": "npm run clean:docs:sol && npx solidity-docgen -o docs/contracts --solc-module solc-0.8.6",
    "clean:docs:sol": "rm -r docs/contracts",
    "test": "npm run build && ts-mocha test",
    "fmt": "ts-standard --fix src task"
  },
  "bin": {
    "feed": "./dist/src/cmd.js"
  },
  "main": "./dist/src/index.js",
  "dependencies": {
    "commander": "^8.0.0",
    "debug": "^4.3.2",
    "dpack": "github:dapphub/dpack",
    "ethers": "^5.4.6",
    "ethers-eip712": "^0.2.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "hardhat": "^2.4.3",
    "solc-0.8.6": "npm:solc@^0.8.6",
    "solidity-docgen": "^0.5.13",
    "ts-mocha": "^8.0.0",
    "ts-standard": "^10.0.0",
    "typescript": "^4.4.2"
  }
}